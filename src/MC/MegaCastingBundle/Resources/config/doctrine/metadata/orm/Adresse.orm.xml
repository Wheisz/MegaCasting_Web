<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Adresse" table="Adresse">
    <unique-constraints>
      <unique-constraint name="UK_Adresse" columns="Numero,Rue,CodePostal,Ville"/>
    </unique-constraints>
    <id name="id" type="bigint" column="Id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="numero" type="integer" column="Numero" nullable="false"/>
    <field name="rue" type="string" column="Rue" length="250" nullable="false"/>
    <field name="codepostal" type="integer" column="CodePostal" nullable="false"/>
    <field name="ville" type="string" column="Ville" length="100" nullable="false"/>
  </entity>
</doctrine-mapping>
